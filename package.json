{
  "name": "opencode-x",
  "version": "4.0.0-alpha.1",
  "description": "OpenCode-X - Sea-Themed AI Agent Harness. Multi-Model Orchestration with TASK_PATH Workflow Engine and Specialized Sea Creature Agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "opencode-x": "./dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./google-auth": {
      "types": "./dist/google-auth.d.ts",
      "import": "./dist/google-auth.js"
    },
    "./schema.json": "./dist/opencode-x.schema.json"
  },
  "scripts": {
    "build": "bun build src/index.ts src/google-auth.ts --outdir dist --target bun --format esm --external @ast-grep/napi && tsc --emitDeclarationOnly && bun build src/cli/index.ts --outdir dist/cli --target bun --format esm --external @ast-grep/napi && bun run build:schema",
    "build:schema": "bun run script/build-schema.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "keywords": [
    "opencode",
    "opencode-x",
    "plugin",
    "kraken",
    "maelstrom",
    "abyssal",
    "nautilus",
    "coral",
    "siren",
    "leviathan",
    "poseidon",
    "scylla",
    "agents",
    "ai",
    "llm",
    "task-path",
    "workflow"
  ],
  "author": "OpenCode-X Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leviathofnoesia/opencode-x.git"
  },
  "bugs": {
    "url": "https://github.com/leviathofnoesia/opencode-x/issues"
  },
  "homepage": "https://github.com/leviathofnoesia/opencode-x#readme",
  "dependencies": {
    "@ast-grep/cli": "^0.40.0",
    "@ast-grep/napi": "^0.40.0",
    "@clack/prompts": "^0.11.0",
    "@code-yeongyu/comment-checker": "^0.6.1",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@openauthjs/openauth": "^0.4.3",
    "@opencode-ai/plugin": "^1.1.1",
    "@opencode-ai/sdk": "^1.1.1",
    "commander": "^14.0.2",
    "glob": "^11.0.0",
    "hono": "^4.10.4",
    "js-yaml": "^4.1.1",
    "jsonc-parser": "^3.3.1",
    "open": "^11.0.0",
    "picocolors": "^1.1.1",
    "picomatch": "^4.0.2",
    "xdg-basedir": "^5.1.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/picomatch": "^3.0.2",
    "bun-types": "latest",
    "typescript": "^5.7.3"
  },
  "trustedDependencies": [
    "@ast-grep/cli",
    "@ast-grep/napi",
    "@code-yeongyu/comment-checker"
  ]
}
